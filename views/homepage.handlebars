<h3 class="page-title">Techie Blog Feed</h3>
<section>
    <ul class="feed-list">
        <div class="divider"></div>
        {{#each posts as |post|}}
        <li>
            <div class="feed-post">
                <p class="post-byline"><span class="post-user">{{user.user_name}}</span> | {{format_date created_at}} ({{timeSince created_at}})</p>
                <a href='/post/{{id}}'><p class="post-title">&gt; {{title}}</p></a>
            </div>
            <div class="divider"></div>
        </li>
        {{/each}}
    </ul>
</section>
